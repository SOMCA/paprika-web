      #set($titlename=$title)
		  #if($title.length()>15)
         #set($titlename="$title.substring(0,16)"+"..")
         #end
      <li>

        <a href="#" ondblclick="menuclick('title$velocityMenu')"><i class="fa fa-bar-database fa-fw"></i>$titlename<span class="fa arrow"></span></a>
        <ul class="nav nav-second-level">

          <li>
            <a href="#" onclick="menuclick('title$velocityMenu');return false;">  $msg.get("INDEX_UL_VIEW")</a>
          </li>
          <form id="forTitle$velocityMenu" style="visibility: hidden; width: 1px; height: 1px" method="post" action="/index/">
